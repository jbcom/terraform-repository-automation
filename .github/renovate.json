{
  "extends": [
    "config:base",
    ":preserveSemverRanges"
  ],
  "labels": ["auto-update"],
  "dependencyDashboard": true,
  "dependencyDashboardAutoclose": true,
  "docker": {
    "enabled": true
  },
  "terraform": {
    "ignorePaths": ["examples/**"]
  },
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  }
}